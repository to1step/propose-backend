{
  "name": "@to1step/propose-backend",
  "version": "1.2.0",
  "description": "",
  "main": "dist/src/lib/server.js",
  "types": "src/types.d.ts",
  "author": "rkdden, gkqkehs7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/to1step/propose-backend.git"
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,js}\"",
    "lint:fix": "eslint --fix \"./src/**/*.{ts,js}\"",
    "lint-staged": "lint-staged",
    "seed": "npm run build && npm run api-docs && cross-env NODE_ENV=seed node dist/src/server.js",
    "test": "NODE_ENV=test jest --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "api-docs": "swagger-cli bundle src/utilies/swaggerDocs/docs.yaml --outfile dist/src/swagger.yaml --type yaml",
    "api-docs-local": "swagger-cli bundle src/utilies/swaggerDocs/docs.yaml --outfile src/swagger.yaml --type yaml",
    "start_local": "npm run api-docs-local && cross-env NODE_ENV=local && nodemon",
    "start_dev": "npm run build && npm run api-docs && cross-env NODE_ENV=development node dist/src/server.js"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.9",
    "aws-sdk": "^2.1366.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "7.0.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "redis": "4.6.5",
    "swagger-cli": "^4.0.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
