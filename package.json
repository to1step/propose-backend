{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/lib/app.js",
  "author": "rkdden, gkqkehs7",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "lint-staged": "lint-staged",
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start_local": "npm run build && cross-env NODE_ENV=local node dist/src/lib/app.js",
    "start_dev": "npm run build && cross-env NODE_ENV=development node dist/src/lib/app.js",
    "start_live": "npm run build && cross-env NODE_ENV=production node dist/src/lib/app.js"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "4.18.2",
    "helmet": "^6.0.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
