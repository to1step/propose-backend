{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Debug Express",
      "request": "launch",
      "program": "${workspaceFolder}/src/index.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "restart": true,
      "runtimeExecutable": "nodemon"
    }
  ]
}
